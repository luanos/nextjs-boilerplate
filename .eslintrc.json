{
  "extends": "next/core-web-vitals",
  "plugins": ["@typescript-eslint", "unused-imports"],
  "rules": {
    "import/no-duplicates": "warn",
    "import/order": [
      "warn",
      {
        "groups": [
          "builtin",
          "external",
          ["internal", "sibling", "index", "parent"],
          "type",
          "object"
        ],
        "newlines-between": "always",
        "alphabetize": {
          "order": "asc"
        }
      }
    ],
    "@typescript-eslint/consistent-type-imports": "error"
  }
}
